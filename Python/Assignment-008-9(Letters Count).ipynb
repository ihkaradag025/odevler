    "def say (c):\n",
    "    k=[]\n",
    "    v=[]\n",
    "    sayac=0 \n",
    "    cumle = list(c)\n",
    "    for b in cumle:\n",
    "        if not b in k:\n",
    "            sayac=cumle.count(b)\n",
    "            v.append(sayac)\n",
    "            k.append(b)        \n",
    "    sozluk=dict(zip(k,v))\n",
    "    print(sozluk)\n",
    "\n",
    "say(input(\"Bir Cümle Giriniz :\"))"
   ]
